            "type": "python",
            "request": "launch",
            "program": "src/active_learning_from_parquet.py",
            "console": "integratedTerminal",
            "args": [
                "--labelled_source", "/phil/plains_wanderer_labelled/tiny_sample/",
                "--model_output_file", "/output/pw_model.keras",
